/Desktop/vitWork/node/project/kom/kom/src/components/Loading.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/components/ErrorMessage.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/components/Cocktails.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/utils/links.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/components/BtnBottom.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/components/Slider.js",["231"],"import React, { useState, useEffect } from \"react\";\nimport Slide from \"./Slide\";\nimport data from \"../data\";\nconst Slider = () => {\n  const [recensioni, _] = useState(data);\n  const [active, setActive] = useState(0);\n\n  //Passa alla prossima slide\n  const prossimaSlide = () => {\n    setActive((prevValue) => {\n      if (prevValue + 1 > recensioni.length - 1) {\n        return 0;\n      } else {\n        return prevValue + 1;\n      }\n    });\n  };\n\n  //passa alla precendete slide\n  const precedenteSlide = () => {\n    setActive((prevValue) => {\n      if (prevValue - 1 < 0) {\n        return recensioni.length - 1;\n      } else {\n        return prevValue - 1;\n      }\n    });\n  };\n\n  //Cambia automaticamente dopo 4 secondi la recensione\n  useEffect(() => {\n    const timer = setTimeout(() => {\n      setActive((prevValue) => {\n        if (prevValue + 1 === recensioni.length) {\n          return 0;\n        } else {\n          return prevValue + 1;\n        }\n      });\n    }, 4000);\n    //Elimina timout prima prima di attivare il prossimo\n    return () => clearTimeout(timer);\n    // eslint-disable-next-line react-hooks/exhaustive-deps\n  }, [active]);\n  return (\n    <div className=\"container slider\">\n      {recensioni.map((recensione, index) => {\n        //Cambio classe in base alla posizione dell'elemento rispetto a quello attivo\n        let positionClass = \"\";\n        if (index === active) {\n          positionClass = \"active\";\n        } else if (active === index + 1 || index === recensioni.length - 1) {\n          positionClass = \"prev\";\n        } else {\n          positionClass = \"next\";\n        }\n        return (\n          <Slide key={recensione.id} {...recensione} classes={positionClass} />\n        );\n      })}\n      <br/><br/>\n      <div className=\"btn-group slider-btn-group\">\n        <button className=\"btn btn-slider prev-slide\" onClick={precedenteSlide}>\n          prev\n        </button>\n        <button className=\"btn btn-slider next-slide\" onClick={prossimaSlide}>\n          next\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default Slider;\n","/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/data.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/components/Slide.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/utils/starCreator.js",[],"/home/vittorio/Desktop/vitWork/node/project/kom/kom/src/utils/firebase.js",["232"],"// Import the functions you need from the SDKs you need\nimport { initializeApp } from \"firebase/app\";\nimport { getAnalytics } from \"firebase/analytics\";\n// TODO: Add SDKs for Firebase products that you want to use\n// https://firebase.google.com/docs/web/setup#available-libraries\n\n// Your web app's Firebase configuration\n// For Firebase JS SDK v7.20.0 and later, measurementId is optional\nconst firebaseConfig = {\n  apiKey: \"AIzaSyD8UX_Y0TZ4xH8YpgOyS-InuDjY-nZ5crc\",\n  authDomain: \"komoving-96b19.firebaseapp.com\",\n  projectId: \"komoving-96b19\",\n  storageBucket: \"komoving-96b19.appspot.com\",\n  messagingSenderId: \"197068924864\",\n  appId: \"1:197068924864:web:d55fa62145ee0b9b98ca25\",\n  measurementId: \"G-1KYBK72PET\"\n};\n\n// Initialize Firebase\nconst app = initializeApp(firebaseConfig);\nconst analytics = getAnalytics(app);","/home/vittorio/Desktop/vitWork/node/project/kom/kom 1.1/src/index.js",["233"],["234","235"],"/home/vittorio/Desktop/vitWork/node/project/kom/kom 1.1/src/App